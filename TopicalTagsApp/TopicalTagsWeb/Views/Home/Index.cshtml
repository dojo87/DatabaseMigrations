@{
    ViewData["Title"] = "Home Page";
}

    <ul>
        @foreach (Topic topic in Model.All)
        {
            <li>@topic.Title <a href="@topic.Url" title="@topic.Title">>> GOTO</a> 
                @foreach (Tag tag in topic.Tags)
                {
                    <span class="badge badge-primary">@tag.Name</span>
                }
            </li>
        }

    </ul>